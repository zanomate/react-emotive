{
  "name": "react-emotive",
  "version": "1.2.0",
  "description": "Wrapper for 'Emotive' library to use in React projects",
  "author": "Zanomate",
  "license": "ISC",
  "main": "react-emotive.ts",
  "homepage": "https://github.com/zanomate/react-emotive#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zanomate/react-emotive.git"
  },
  "bugs": {
    "url": "https://github.com/zanomate/react-emotive/issues"
  },
  "scripts": {
    "clean": "shx rm -rf dist/*",
    "generate": "ts-node --project src/tsconfig.json -r tsconfig-paths/register src/index.ts",
    "rollup": "rollup -c",
    "types": "tsc -d --outDir dist build/react-emotive.ts",
    "cp": "shx cp build/package.json dist && shx cp README.md dist",
    "build": "npm run clean && npm run generate && npm run types && npm run rollup && npm run cp"
  },
  "peerDependencies": {
    "filter-invalid-dom-props": "^2.0.0",
    "global": "^4.3.2",
    "react": "^16.6.0",
    "emotive": "^1.2.1",
    "styled-components": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@types/filter-invalid-dom-props": "^2.0.0",
    "@types/node": "^10.12.2",
    "@types/prettier": "^1.13.2",
    "@types/react": "^16.4.18",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-syntax-parser": "^1.5.1",
    "csstype": "^2.5.7",
    "emotive": "^1.2.1",
    "filter-invalid-dom-props": "^2.0.0",
    "fs": "0.0.1-security",
    "global": "^4.3.2",
    "mdn-data": "^2.0.0",
    "prettier": "^1.14.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-babel-minify": "^6.1.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.17.2",
    "shx": "^0.3.2",
    "source-map-loader": "^0.2.4",
    "styled-components": "^4.0.3",
    "ts-loader": "^5.1.0",
    "ts-node": "^7.0.1",
